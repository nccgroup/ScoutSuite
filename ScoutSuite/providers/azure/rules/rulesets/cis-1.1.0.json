{
    "about": "This ruleset covers most of the recommendations from the CIS Microsoft Azure Foundation v1.1.0.",
    "rules": {
        "appservice-outdated-version-dotnet.json": [
            {
                "enabled": true,
                "level": "warning"
            }
        ],
        "appservice-outdated-version-java.json": [
            {
                "enabled": true,
                "level": "warning"
            }
        ],
        "appservice-outdated-version-php.json": [
            {
                "enabled": true,
                "level": "warning"
            }
        ],
        "appservice-outdated-version-python.json": [
            {
                "enabled": true,
                "level": "warning"
            }
        ]
    }
}
