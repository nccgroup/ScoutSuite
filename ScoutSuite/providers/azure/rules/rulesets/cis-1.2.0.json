            {
                "enabled": true,
                "level": "warning"
            }
        ],
        "postgresql-database-servers-log-connections-not-on.json": [
            {
                "enabled": true,
                "level": "warning"
            }
        ],
        "postgresql-database-servers-log-disconnections-not-on.json": [
